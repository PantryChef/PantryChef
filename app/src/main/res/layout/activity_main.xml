<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.pantrychefapp.pantrychef.MainActivity">

    <Button
        android:id="@+id/my_recipes_btn"
        android:layout_width="159dp"
        android:layout_height="52dp"
        android:layout_marginTop="29dp"
        android:onClick="getMyRecipes"
        android:text="@string/my_recipes"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/my_pantry_btn"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1"
        tools:layout_editor_absoluteX="111dp"/>

    <Button
        android:id="@+id/my_pantry_btn"
        android:layout_width="159dp"
        android:layout_height="52dp"
        android:layout_marginTop="100dp"
        android:onClick="getMyPantry"
        android:text="@string/my_pantry"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1"
        tools:layout_editor_absoluteX="96dp"/>

</android.support.constraint.ConstraintLayout>
